package Emp;

public class Switch {

	public static void main(String[] args) {
	
		char switchChar = 'C' ; 
		
		switch(switchChar) { 
			case 'A':
				System.out.println("Value is A");
				break;
			case 'B': 
				System.out.println("Value is B");
				break;
			case 'C': case 'D': case'E':
				System.out.println("Value is C");
				break;
				
				default:
					System.out.println("None of these");
				break;
		}
	}
		
}
